{
    "name": "theartoflogic/doctrine-tree-extension",
    "type": "library",
    "description": "Adds tree behaviors to your doctrine entities.",
    "keywords": [
        "theartoflogic",
        "doctrine2",
        "behaviors",
        "extensions",
        "tree",
        "nested set",
        "closure"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sarah Ryan",
            "email": "sarah@theartoflogic.com",
            "homepage": "http://theartoflogic.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/orm": ">=2.2.3,<2.4-dev"
    },
    "require-dev": {
        "symfony/config": "dev-master",
        "symfony/http-kernel": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "TheArtOfLogic\\DoctrineTreeExtension": "lib/" }
    }
}